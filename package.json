{
  "name": "tategaki-ni-narou",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && NODE_ENV=production vite build ; cd dist ; zip ../tategaki-ni-narou.zip -r *",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "jquery": "^3.7.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^1.0.14",
    "@types/chrome": "^0.0.243",
    "@types/jquery": "^3.5.17",
    "@types/prettier": "^3",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.1"
  },
  "volta": {
    "node": "20.3.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "printWidth": 120
  },
  "eslintConfig": {
    "env": {
      "webextensions": true,
      "browser": true
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {},
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "node_modules"
    ]
